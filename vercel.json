{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "functions": {
    "api/upload": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
} 